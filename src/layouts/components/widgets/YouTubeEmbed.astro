---
type Props = {
  url: string;
  label: string;
};
const { url, label } = Astro.props;
---

<div class="aspect-video">
  <iframe
    class="w-full h-full"
    loading="lazy"
    src={url}
    title={label}
    frame-border="0"
    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
    allowfullscreen
  >
  </iframe>
</div>
